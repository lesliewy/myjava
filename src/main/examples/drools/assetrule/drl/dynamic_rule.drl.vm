package rules.enum

import examples.drools.assetrule.rule.EnumRule
import examples.drools.assetrule.function.EnumFunctionImpl
import examples.drools.assetrule.data.DroolsExecuteContext

rule "dynamic Rule: common"
    salience 60
    when
        ruleCondition:DynamicRule()
        context:DroolsExecuteContext()
        dynamicFunction: DynamicFunctionImpl()
    then
        dynamicFunction.execute(ruleCondition, context);
    end

