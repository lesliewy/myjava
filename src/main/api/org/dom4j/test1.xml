<?xml version="1.0" encoding="UTF-8" ?>
<ROOT>
   <INFO>
      <DESC>千惠集-账单支付-电子账单显示格式-一级菜单</DESC>
      <VERSION>0.0.1</VERSION>
   </INFO>
   <ROW>
      <PARAM>
         <PARAM-NAME>账单类型名称</PARAM-NAME>          <!--  信用卡账单、保险账单 -->
         <ETF-NAME>BILL_NM</ETF-NAME>
      </PARAM>
      <PARAM>
         <PARAM-NAME>账单笔数</PARAM-NAME>
         <ETF-NAME>BILL_NUM</ETF-NAME>
         <DISPLAY>共BILL_NUM条账单信息</DISPLAY>
      </PARAM>
      <PARAM>
         <PARAM-NAME>LOGO</PARAM-NAME>
         <PARAM-URL>logo url</PARAM-URL>
      </PARAM>
   </ROW>
</ROOT>
